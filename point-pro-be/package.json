{
  "name": "point-pro-be",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --exit-child --respawn --files src/server.ts",
    "add-seeds:prod": "ts-node-dev --files prisma/migrate.ts",
    "update-reservations": "ts-node-dev --files prisma/updateReservations.ts",
    "update-periods": "ts-node-dev --files prisma/updatePeriods.ts",
    "build": "tsc -p .",
    "build:prod": "cross-env NODE_ENV=production npx prisma migrate deploy && npx prisma generate && tsc -p .",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "testquery": "npx ts-node prisma/script.ts"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^7.3.0",
    "express-session": "^1.17.3",
    "imgur": "^2.4.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-ecpay-aio": "^0.2.3",
    "nodemailer": "^6.9.3",
    "pg": "^8.10.0",
    "ramda": "^0.29.0",
    "socket.io": "^4.6.1",
    "tslib": "^2.5.3",
    "typescript": "^5.0.3",
    "uuid": "^9.0.0",
    "yup": "^1.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.19.33",
    "@types/nodemailer": "^6.4.8",
    "@types/pg": "^8.6.6",
    "@types/ramda": "^0.29.2",
    "@types/uuid": "^9.0.2",
    "http-status-codes": "^2.3.0",
    "prisma": "^5.14.0",
    "supabase": "^1.49.4",
    "ts-node-dev": "^2.0.0"
  }
}
